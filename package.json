{
  "name": "coub-downloader",
  "version": "2.0.1",
  "description": "Download all favorite coubs",
  "keywords": [
    "coub",
    "download"
  ],
  "author": "Anatoliy Oblaukhov",
  "homepage": "https://gitlab.com/anmiles/coub-downloader",
  "repository": "gitlab:anmiles/coub-downloader",
  "license": "MIT",
  "engines": {
    "node": ">=12.22.1"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc && tsc --project ./src/client",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --verbose",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "npm test -- --ci --coverage",
    "test:watch": "npm test -- --watch",
    "test:watch:coverage": "npm test -- --watch --coverage",
    "test:report:coverage": "nyc report --nycrc-path ./coverage.config.js -t ./coverage --report-dir ./coverage",
    "start": "node ./dist/index.js",
    "create": "node ./dist/create.js",
    "migrate": "node ./dist/migrate.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "colorette": "^2.0.19",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@anmiles/eslint-config": "^1.0.6",
    "@types/jest": "^29.5.1",
    "@types/jquery": "^3.5.16",
    "@types/lodash": "^4.14.194",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "eslint-plugin-align-assignments": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
