{
  "name": "coub-downloader",
  "version": "4.0.0",
  "description": "Download favorite and liked coubs into local html file",
  "keywords": [
    "coub",
    "download",
    "videos"
  ],
  "author": "Anatoliy Oblaukhov",
  "homepage": "https://gitlab.com/anmiles/coub-downloader",
  "repository": "gitlab:anmiles/coub-downloader",
  "license": "MIT",
  "engines": {
    "node": ">=18.14.2"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc && tsc --project ./src/client",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --verbose",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "npm test -- --ci --coverage",
    "test:watch": "npm test -- --watch",
    "test:watch:coverage": "npm test -- --watch --coverage",
    "test:report:coverage": "nyc report --nycrc-path ./coverage.config.js -t ./coverage --report-dir ./coverage",
    "start": "node ./dist/index.js",
    "create": "node ./dist/create.js"
  },
  "dependencies": {
    "@anmiles/downloader": "^3.0.1",
    "@anmiles/logger": "^4.0.1",
    "@anmiles/prototypes": "^5.1.1",
    "@anmiles/sleep": "^2.0.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@anmiles/eslint-config": "^2.0.2",
    "@types/jest": "^29.5.8",
    "@types/jquery": "^3.5.27",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-plugin-align-assignments": "^1.1.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
