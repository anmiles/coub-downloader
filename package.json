{
  "name": "coub-download",
  "version": "1.0.1",
  "description": "Download all favorite coubs",
  "keywords": [
    "coub",
    "download"
  ],
  "author": "Anatoliy Oblaukhov",
  "homepage": "https://gitlab.com/anmiles/coub-download",
  "repository": "gitlab:anmiles/coub-download",
  "license": "MIT",
  "engines": {
    "node": ">=12.22.1"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc && tsc --project ./src/client",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --fix --ext .js,.ts .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:watch": "jest --watch",
    "test:watch:coverage": "jest --watch --coverage",
    "test:report:coverage": "nyc report --nycrc-path ./coverage.config.js -t ./coverage --report-dir ./coverage",
    "start": "node ./dist/index.js",
    "create": "node ./dist/create.js",
    "migrate": "node ./dist/migrate.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "colorette": "^2.0.19",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/jquery": "^3.5.14",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-plugin-align-assignments": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "nyc": "^15.1.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  }
}
